    "@nestjs/schedule": "^2.1.0",
    "@nestjs/serve-static": "^3.0.0",
    "@typegoose/typegoose": "^10.1.0",
    "app-root-path": "^3.1.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.29.3",
    "fs-extra": "^11.1.0",
    "mongoose": "6.9.0",
    "nestjs-typegoose": "^7.1.38",
    "passport": "^0.6.0",
