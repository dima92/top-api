    "sharp": "^0.31.3",
    "telegraf": "^4.11.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.1.9",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.1",
    "@types/app-root-path": "^1.2.4",
		"@types/bcryptjs": "^2.4.2",
		"@types/cron": "^2.0.0",
    "@types/express": "^4.17.16",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "29.4.0",
    "@types/multer": "^1.4.7",
    "@types/node": "18.11.18",
    "@types/passport-jwt": "^3.0.8",
		"@types/sharp": "^0.31.1",
    "@types/supertest": "^2.0.12",
    "@types/xml2js": "^0.4.11",
    "jest": "29.4.1",
    "prettier": "^2.8.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "4.1.2",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
